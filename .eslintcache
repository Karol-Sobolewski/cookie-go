ExchangeRate(euro));\n          // console.log(euro);\n        });\n    };\n    setTimeout(() => {\n      loadData();\n    }, 500);\n  }, []);\n\n  return (\n    <>\n      {loaded ? (\n        <div className={styles.app}>\n          <BrowserRouter>\n            <ScrollToTop />\n            <MainLayout>\n              <AnimatedSwitch\n                atEnter={{ opacity: 0 }}\n                atLeave={{ opacity: 0 }}\n                atActive={{ opacity: 1 }}\n                className={styles.switchWrapper}\n              >\n                <Route exact path=\"/\" component={() => <HomePage />} />\n                <Route\n                  exact\n                  path=\"/en\"\n                  component={() => <HomePage language=\"en\" />}\n                />\n                <Route\n                  exact\n                  path=\"/pl\"\n                  component={() => <HomePage language=\"pl\" />}\n                />\n                <Route exact path=\"/about\" component={() => <About />} />\n                <Route\n                  exact\n                  path=\"/cookies\"\n                  component={() => <ProductsPage productName=\"cookies\" />}\n                />\n                <Route\n                  exact\n                  path=\"/pastries\"\n                  component={() => <ProductsPage productName=\"pastries\" />}\n                />\n                <Route\n                  exact\n                  path=\"/cakes\"\n                  component={() => <ProductsPage productName=\"cakes\" />}\n                />\n                <Route\n                  exact\n                  path=\"/sweets\"\n                  component={() => <ProductsPage productName=\"sweets\" />}\n                />\n                <Route exact path=\"/contact\" component={() => <Contact />} />\n                <Route\n                  exact\n                  path=\"/products/:id\"\n                  component={SingleProductPage}\n                />\n           